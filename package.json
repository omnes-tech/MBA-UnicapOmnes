  "scripts": {
    "compile": "hardhat compile --force",
    "test": "hardhat test",
    "lint": "yarn lint:sol && yarn lint:ts && yarn prettier:check",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --config ./.eslintrc.yml --ignore-path ./.eslintignore --ext .js,.ts .",
    "postinstall": "husky install && yarn typechain",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "deploy:truffle": "yarn hardhat run scripts/deploy.ts --network truffle",
    "deploy:NFT": "yarn hardhat run scripts/deployNFT.ts --network truffle",
    "deploy:job": "yarn hardhat run scripts/deployJob.ts --network truffle",
    "prettier": "prettier --config ./.prettierrc.yml --write \"**/*.{js,json,md,sol,ts,yaml,yml,test.ts}\"",
    "prettier:check": "prettier --check --config ./.prettierrc.yml \"**/*.{js,json,md,sol,ts,yaml,yml}\""
  },
  "dependencies": {
    "@babel/parser": "^7.22.5",
    "@openzeppelin/contracts": "^4.9.1",
